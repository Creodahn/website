<main>
  {{#title-card
    fullTitle=fullTitle
    model=model
    alterTitle="alterTitle"
  }}
    {{#if fullTitle}}
    {{else}}
      <section class="container">
        {{nav-bar
          environment=environment
          isAuthenticated=isAuthenticated
          login="login"
          logout="logout"
        }}
        {{outlet}}
        {{page-footer}}
      </section>
    {{/if}}
  {{/title-card}}
</main>
